{
  "author": "Pierce Moore <me@prex.io>",
  "name": "rex-git",
  "description": "A toolkit built in Node.js that makes it easy to perform git operations recursively on a folder and its subfolders.",
  "homepage": "https://github.com/rex/rex-git",
  "version": "0.1.0",
  "main": "./lib/rex-git.js",
  "bin": {
    "rex-git": "./bin/rex-kickstart",
    "rgit": "./bin/rex-kickstart"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rex/rex-git.git"
  },
  "keywords": [
    "git",
    "folder",
    "recursive",
    "rex"
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "dependencies": {
    "optimist": "*",
    "underscore": "*",
    "async": "*",
    "rex-shell": "*",
    "rex-exec": "*"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=0.10.x"
  },
  "config": {},
  "bugs": {
    "url": "https://github.com/rex/rex-git/issues"
  },
  "scripts": {
    "test": "node ./test/rex-git.test.js"
  },
  "preferGlobal": true
}
